{
  "name": "crdt-module",
  "version": "1.0.0",
  "description": "A starter project to kickstart the development of a CSS module.",
  "keywords": [
    "solid",
    "linked-data",
    "rdf",
    "ldp",
    "starter-module"
  ],
  "license": "MIT",
  "author": " <your@email.com> (https://optional.link/to-your-website/)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:GitHubUserName/RepositoryName.git",
  "bugs": {
    "url": "https://github.com/GitHubUserName/RepositoryName/issues"
  },
  "homepage": "https://github.com/GitHubUserName/RepositoryName#readme",
  "files": [
    "config",
    "dist"
  ],
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/crdt-module",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/crdt-module/^1.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/crdt-module/^1.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/crdt-module/^1.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/crdt-module/^1.0.0/dist/": "dist/"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s src -c dist/components -i .componentsignore",
    "build:ts": "tsc",
    "lint": "eslint . --cache",
    "prepare": "npm run build",
    "start": "npm run build && npm run build && npx community-solid-server -c config/default.json  -m .",
    "test:integration": "jest --coverageReporters text-summary -- test/integration",
    "test:unit": "jest --config=./jest.coverage.config.js test/unit",
    "test:watch": "jest --coverageReporters none --watch test/unit"
  },
  "dependencies": {
    "@rdfjs/data-model": "^1.2.0",
    "@types/arrayify-stream": "^1.0.0",
    "@types/node-fetch": "^2.5.7",
    "@types/rdf-js": "^4.0.0",
    "@types/streamify-array": "^1.0.0",
    "arrayify-stream": "^1.0.0",
    "fs-extra": "^10.0.0",
    "jest-rdf": "^1.7.0",
    "node-fetch": "^2.6.1",
    "rdf-parse": "^1.9.1",
    "rdf-serialize": "^1.2.0",
    "streamify-array": "^1.0.1",
    "to-readable-stream": "^1.0.0",
    "urijs": "^1.19.5",
    "yjs": "^13.5.13"
  },
  "peerDependencies": {
    "@solid/community-server": "^0.8.1"
  },
  "devDependencies": {
    "@solid/community-server": "^1.1.0",
    "@tsconfig/node14": "^1.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "componentsjs-generator": "^2.0.0",
    "eslint": "^7.16.0",
    "eslint-config-es": "^3.26.13",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-tsdoc": "^0.2.10",
    "eslint-plugin-unused-imports": "^1.0.1",
    "husky": "^4.3.6",
    "jest": "^26.0.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.1.3"
  }
}
